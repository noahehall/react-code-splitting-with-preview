{
  "name": "test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npm list firebase-tools@3.14.0 -g || npm install firebase-tools@3.14.0 -g",
    "start:dev": "npm run prebuild:all && NODE_ENV=development webpack-dev-server --host 0.0.0.0 --port 5000",
    "prebuild:all": "mkdirp ./public && rimraf ./public/* && ncp ./src/shared/assets ./public",
    "build:staging": "npm run prebuild:all && NODE_ENV=staging webpack",
    "build:production": "npm run prebuild:all && NODE_ENV=production webpack",
    "deploy:staging": "npm run build:staging && firebase deploy --project staging",
    "deploy:production": "npm run build:production && firebase deploy --project production",
    "bundle-analyzer": "npm run prebuild:all && NODE_ENV=production BUNDLE_ANALYZER=true webpack"
  },
  "engines": {
    "node": "8.x.x"
  },
  "dependencies": {
    "firebase": "4.6.1",
    "firebaseui": "2.4.1",
    "localforage": "1.5.3",
    "nprogress": "0.2.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-loadable": "5.3.1",
    "react-router-dom": "4.2.2",
    "redux-zero": "4.3.1"
  },
  "devDependencies": {
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-dual-import-next": "git+https://github.com/sh-a-v/babel-plugin-dual-import-next.git",
    "babel-plugin-lodash": "3.2.11",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "cache-loader": "1.1.0",
    "css-loader": "0.28.7",
    "css-mqpacker": "6.0.1",
    "extract-css-chunks-webpack-plugin": "2.0.17",
    "file-loader": "1.1.5",
    "flush-css-chunks-webpack-plugin": "0.1.0",
    "html-webpack-plugin": "2.30.1",
    "mkdirp": "0.5.1",
    "ncp": "2.0.0",
    "postcss-loader": "2.0.8",
    "postcss-nested": "2.1.2",
    "postcss-simple-vars": "4.1.0",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.2",
    "script-ext-html-webpack-plugin": "1.8.8",
    "style-ext-html-webpack-plugin": "3.4.3",
    "style-loader": "0.19.0",
    "thread-loader": "1.1.2",
    "webpack": "3.8.1",
    "webpack-bundle-analyzer": "2.9.0",
    "webpack-dev-server": "2.9.4"
  }
}
